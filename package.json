{
  "name": "mah-book-room-api",
  "version": "0.3.1",
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha test/*",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec test/*",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec test/* && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidjosefson/mah-book-room-api"
  },
  "dependencies": {
    "basic-auth": "~1.0.3",
    "body-parser": "~1.13.2",
    "chai": "~3.3.0",
    "cookie-parser": "~1.3.5",
    "cors": "~2.7.1",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "html-to-text": "~1.4.0",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "request": "~2.64.0",
    "serve-favicon": "~2.3.0"
  },
  "engines": {
    "node": "0.12.x"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "~2.4.5",
    "mocha-lcov-reporter": "^1.0.0"
  }
}
